      ],
      "source": [
        "NN = int(input(\"첫째 줄에 숫자 N을 입력하세요: \"))\n",
        "files = [input() for _ in range(N)]\n",
        "\n",
        "result = [file for file in files if file.split('.')[-1] in ('png', 'jpg')]\n",
        "\n",
        "print(result)\n",
        "\n"
      ]
    },
