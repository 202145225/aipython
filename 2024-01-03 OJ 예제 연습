      "cell_type": "code",
      "source": [
        "A, B, C = map(int, input().split())\n",
        "if A >= B and A >= C:\n",
        "    if B >= C:\n",
        "        second_largest = B\n",
        "    else:\n",
        "        second_largest = C\n",
        "elif B >= A and B >= C:\n",
        "    if A >= C:\n",
        "        second_largest = A\n",
        "    else:\n",
        "        second_largest = C\n",
        "else:\n",
        "    if A >= B:\n",
        "        second_largest = A\n",
        "    else:\n",
        "        second_largest = B\n",
        "\n",
        "# 결과 출력\n",
        "print(second_largest)"
